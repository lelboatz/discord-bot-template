{
  "name": "discord-bot-template",
  "version": "0.0.1",
  "description": "A discord.js bot template in TypeScript with a MongoDB and express.js server implementation",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "watch": "tsc -p tsconfig.json -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lelboatz/discord-bot-template.git"
  },
  "author": "lelboatz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lelboatz/discord-bot-template/issues"
  },
  "homepage": "https://github.com/lelboatz/discord-bot-template#readme",
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/express": "^4.17.16",
    "@types/klaw": "^3.0.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.3.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "klaw": "^4.1.0",
    "mongodb": "^5.0.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2"
  }
}
